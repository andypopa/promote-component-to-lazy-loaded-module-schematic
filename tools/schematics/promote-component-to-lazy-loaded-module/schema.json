{
  "$schema": "http://json-schema.org/schema",
  "id": "promote-component-to-lazy-loaded-module-schematic",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "Project name",
      "x-prompt": "Which project?",
      "default": "qwerty"
    },
    "componentClassName": {
      "type": "string",
      "description": "Component class name",
      "x-prompt": "Which component (class name i.e. MyNewComponent)?",
      "default": "MapComponent"
    }
  },
  "required": ["project", "componentClassName"]
}
